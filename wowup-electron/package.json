{
  "name": "wowup",
  "productName": "WowUp",
  "version": "2.20.0",
  "private": true,
  "description": "World of Warcraft addon updater",
  "keywords": [
    "wow",
    "world of warcraft"
  ],
  "homepage": "https://wowup.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/WowUp/WowUp.git"
  },
  "author": {
    "name": "WowUp LLC",
    "email": "zakrn@wowup.io"
  },
  "main": "app/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && ow-electron-builder install-app-deps",
    "install:prod": "npm ci --only=prod --no-optional",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "cross-env BUILD_FLAVOR=wago npm run build:injectenv && npm run electron:serve-tsc && ng build --base-href ./",
    "build:injectenv": "node app-env/inject-env.js",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c dev",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "cross-env BUILD_FLAVOR=wago npm run build:injectenv && wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "electron:build:local": "npm run build:prod && electron-builder build -c electron-build/electron-builder-local.json",
    "electron:publish": "npm run lint && npm run build:prod && electron-builder -c electron-build/electron-builder.json build --publish always",
    "electron:publish:never": "npm run electron:build && electron-builder --publish never",
    "electron:publish:local": "npm run lint && npm run build:dev && npx electron-builder -c electron-build/electron-builder-local.json --publish never",
    "ow:start": "npm-run-all -p ow:electron:serve ow:ng:serve",
    "ow:build": "cross-env BUILD_FLAVOR=ow npm run build:injectenv && npm run electron:serve-tsc && ng build --base-href ./",
    "ow:build:dev": "npm run ow:build -- -c ow-dev",
    "ow:build:prod": "npm run ow:build -- -c ow-production",
    "ow:ng:serve": "ng serve -c ow-dev",
    "ow:electron:serve": "cross-env BUILD_FLAVOR=ow npm run build:injectenv && wait-on tcp:4200 && npm run electron:serve-tsc && npx ow-electron . --serve",
    "ow:electron:local": "npm run build:prod && npx ow-electron .",
    "ow:electron:build": "npm run build:prod && ow-electron-builder build",
    "ow:electron:build:local": "npm run build:prod && ow-electron-builder build -c electron-build/electron-builder-local.json",
    "ow:electron:publish": "npm run lint && npm run ow:build:prod && ow-electron-builder -c electron-build/electron-builder-local-ow.json build --publish always",
    "ow:electron:publish:vanilla": "npm run lint && npm run ow:build:prod && electron-builder -c electron-build/electron-builder-local-ow.json build --publish always",
    "ow:electron:publish:never": "npm run electron:build && ow-electron-builder --publish never",
    "ow:electron:publish:local": "npm run lint && npm run ow:build:dev && npx ow-electron-builder -c electron-build/electron-builder-local-ow.json --publish never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "test:locales": "ng test --watch=false --include='src/locales.spec.ts'",
    "test:customprotocol:": "npx electron . \"curseforge://install?addonId=3358^^^&fileId=3240590\"",
    "ow:test:customprotocol:": "npx ow-electron . \"curseforge://install?addonId=14154&fileId=4073235\"",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "i18n": "sync-i18n --files ./src/assets/i18n/*.json --primary en --space 2 --finalnewline --lineendings CRLF --languages cs de es fr it nb pt ru zh zh-TW ko pl",
    "check-i18n": "npm run i18n -- --check",
    "pretty": "npx prettier --write . && ng lint --fix",
    "find-broken-test": "node ./test-fixer.js --find-break",
    "restore-tests": "node ./test-fixer.js --repair",
    "package:prod": "npx gulp package",
    "package:local": "npx gulp packageLocal"
  },
  "browserslist": [
    "chrome 98"
  ],
